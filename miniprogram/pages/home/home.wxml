<!--pages/home/home.wxml-->
<view>
  <swiper 
    indicator-dots="{{indicatorDots}}" 
    autoplay="{{autoplay}}" 
    interval="{{interval}}" 
    duration="{{duration}}" 
    previous-margin="{{previousMargin}}px" 
    next-margin="{{nextMargin}}px">
    <block wx:for="{{imgs}}" wx:key="index">
      <swiper-item>
        <image src="{{item}}"></image>
      </swiper-item>
    </block>
  </swiper>
</view>
<my-sharing prop-list="{{list}}"></my-sharing>

